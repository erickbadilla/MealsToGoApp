{
  "extends": "@react-native-community",
  "rules": {
    "quotes": [2, "double", { "avoidEscape": true }],
    "comma-dangle": "off"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "ignorePatterns": ["/functions/lib/**"]
}
